<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <objectAnimator
        android:interpolator="@anim/alpha_curve"
        android:duration="@integer/expansion_fade_in_duration_ms"
        android:delay="@integer/expansion_fade_in_delay_ms"
        android:valueFrom="0"
        android:valueTo="1"
        android:propertyName="alpha"/>
    <objectAnimator
        android:interpolator="@anim/enter_curve"
        android:duration="@integer/expansion_row_slide_up_duration_ms"
        android:valueFrom="@dimen/expansion_row_slide_up"
        android:valueTo="0"
        android:propertyName="translationY"/>
</set>
