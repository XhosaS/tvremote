<?xml version="1.0" encoding="utf-8"?>
<androidx.leanback.widget.NonOverlappingLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <android.widget.GridLayout
        android:id="@+id/grid_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/katniss_browse_padding_start"
        android:layout_marginEnd="@dimen/katniss_browse_padding_end"/>
</androidx.leanback.widget.NonOverlappingLinearLayout>
